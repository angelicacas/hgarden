  <!-- CONOCENOS -->
<!--contactanos-->
  <h2>CONTACTANOS</h2>
  <div class="col-md-4 offset-md-2">
    <form action="/" class="mb-3" method="post" id="contact_form">
      <div class="form-group">
        <div class="row">
          <div class="col pb-3">
            <label for="Name" class="form-label">Nombre</label>
            <input type="text" name="Name" id="Name" class="form-control" placeholder="Nombre">
          </div>
          <div class="col">
            <label for="lastName" class="form-label">Apellido</label>
            <input type="text" name="lastName" class="form-control" id="lastName" placeholder="Apellido">
          </div>
        </div>
        <div class="row">
          <div class="col pb-3">
            <label for="email">Email</label>
            <input type="email" name="email" id="email" placeholder="Email" class="form-control">
          </div>
        </div>
        <div class="row">
          <div class="col pb-3">
            <label for="Consulta" class="d-block">Consulta</label>
            <textarea name="Consulta" id="Consulta" cols="40" rows="7" placeholder="Su consulta"></textarea>
          </div>
        </div>
        <button class="btn btn-main">Enviar</button>
        <span id="feedback">
          {{!-- {{#if message}}
          <p>{{message}}</p>
          {{/if}} --}}
        </span>
      </div>
    </form>

  </div>

</div>

</section>
<!-- FIN CONTACTANOS -->

